<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="16dp"
        android:paddingBottom="16dp"
        android:paddingStart="8dp"
        android:paddingEnd="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/list_report_user_wise_customer"
            style="@style/userWiseTextView"
             />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/list_report_user_wise_address"
            style="@style/userWiseTextView"
             />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/list_report_user_wise_MbNo"
            style="@style/userWiseTextView"
             />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/list_report_user_wise_brand"
            style="@style/userWiseTextView"/>

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/list_report_user_wise_remark"
            style="@style/userWiseTextView" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/list_report_user_wise_counter_approx"
            style="@style/userWiseTextView" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/list_report_user_wise_response"
            style="@style/userWiseTextView" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/list_report_user_wise_fname"
            style="@style/userWiseTextView" />

    </androidx.appcompat.widget.LinearLayoutCompat>

    <View
        android:id="@+id/view"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/list_item_divider_color"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>